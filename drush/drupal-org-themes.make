; drush make build file for drupal.org packaging.
; This file defines Drupal contrib themes.

api = 2
core = 7.x

; Rubik
; Rubik is a clean admin theme.
projects[rubik][version] = 4.2

; Tao
; Tao is a base theme for Drupal that is all about going with the flow.
; Required by Rubik theme.
projects[tao][version] = 3.1


; Zen
; Zen is a powerful, yet simple, HTML5 starting theme with a responsive, mobile-first grid design.
projects[zen][version] = 5.5
